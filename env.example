# Copy this file to .env and update the values

# Frontend Supabase Configuration (Legacy - not used in current implementation)
VITE_SUPABASE_PROJECT_ID="your-supabase-project-id"
VITE_SUPABASE_PUBLISHABLE_KEY="your-supabase-anon-key"
VITE_SUPABASE_URL="https://your-project.supabase.co"

# Database Configuration (MongoDB Atlas or local)
MONGODB_URI="mongodb+srv://username:password@cluster.mongodb.net/?retryWrites=true&w=majority&appName=APP_NAME"

# Session Security (CHANGE IN PRODUCTION!)
SESSION_SECRET="please_change_this_long_random_secret"

# CORS Configuration
CORS_ORIGIN="http://localhost:5173"

# HTTPS Enforcement (set to true in production)
HTTPS_ONLY="false"

# Server Port
PORT=3001

# Node Environment
NODE_ENV=development
